					fs++;
				}
				try {
					document.getElementsByTagName("html")[0].style.fontSize = fs + "px";
					document.getElementsByTagName("body")[0].style.fontSize = fs + "px";
				} catch(e) {}
			},
			1000);
		}
	}
})();
