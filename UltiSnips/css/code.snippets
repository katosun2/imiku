snippet cssdh "css3动画"
animation: $1 ${2:.5s};
-moz-animation: $1 $2;
-webkit-animation: $1 $2;
-o-animation: $1 $2;
@keyframes ${1:myanime} {
	from {background: red;}
	to {background: yellow;}
}
@-webkit-keyframes $1 {
	from {background: red;}
	to {background: yellow;}
}
@-moz-keyframes $1 {
	from {background: red;}
	to {background: yellow;}
}
@-o-keyframes $1 {
	from {background: red;}
	to {background: yellow;}
}
endsnippet


snippet csscb "css3 checkbox"
:root input[type="checkbox"] {
	display:none;
}
:root input[type="checkbox"] + label:before {
	display:inline-block;
	width:28px;
	height:28px;
	margin:-1px 4px 0 0;
	vertical-align:middle;
	background:url() left top no-repeat;
	background-size:contain;
	cursor:pointer;
	content:"";
}
:root input[type="checkbox"]:checked + label:before {
	background-position:-38px;
	background-size:contain;
	content:"";
}
endsnippet


snippet cssrd "css3 radio"
:root input[type="radio"]{
	display:none;
}
:root input[type="radio"] + label:before {
	display:inline-block;
	margin-right:0.5em;
	width:1.2em;
	height:1.2em;
	vertical-align:middle;
	background:url() no-repeat;
	background-size:contain;
	cursor:pointer;
	content:"";
}
:root input[type="radio"]:checked + label:before {
	background:url() no-repeat;
	background-size:contain;
	content:"";
}
endsnippet

