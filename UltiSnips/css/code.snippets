snippet iconfont "图标字体"
/*define
use in css : .iconfont{font-family:"${1:iconfont}"; font-size:16px;font-style:normal;}
use in css : .iconfont{-webkit-font-smoothing: antialiased;}
use in html : <i class="iconfont">&#33</i>
*/
@font-face {font-family: '$1';
	src: url('$1.eot'); /* IE9*/
	src: url('$1.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
	url('$1.woff') format('woff'), /* chrome、firefox */
	url('$1.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
	url('$1.svg#iconfont') format('svg'); /* iOS 4.1- */
}
endsnippet

